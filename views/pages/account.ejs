<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

<% include ../partials/nav.ejs %>

<div class="container">
<h2>Accounts</h2>

<ul>
    <table border=1>
        <tr>
            <th>Account ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Region</th>
            <th>Age</th>
        </tr>
        <% result.rows.forEach(function(row) { %>
            <tr>
                <td><%= row.accid %></td>
                <td><%= row.fname %></td>
                <td><%= row.lname %></td>
                <td><%= row.region %></td>
                <td><%= row.age %></td>
            </tr>
        <% }); %>
    </table>

</ul>

</div>

</body>
</html>
